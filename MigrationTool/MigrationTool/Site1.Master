                            <td width="10%" style="text-align: left; height: 50px; word-wrap: unset">
                                <asp:Image ID="Image2" runat="server" ImageUrl="~/Images/vm-logo.png" />

                            </td>
                    </table>
                </div>

                <div runat="server" id="divMenu">
                    <table width="100%" style="background-color: white;">
                        <tr>
                            <td width="3%" style="text-align: left; word-wrap: unset">
                                <asp:ImageButton ID="Image1" runat="server" Height="50px" ImageUrl="~/Images/MenuLogo.jpg" CausesValidation="false" OnClientClick="document.location.href = 'dashboard.aspx'; return false" />
                            </td>
                            <td width="75%" style="text-align: center">
                                <div class="navbar">
                                    <a href="Dashboard.aspx">Dashboard</a>
                                    <div class="dropdown" runat="server" id="divAdmin1">
                                        <button class="dropbtn" disabled>
                                            Admin 
                                <i class="fa fa-caret-down"></i>
                                        </button>
                                        <div class="dropdown-content">
                                            <a href="DBCreation.aspx">Create Project</a>
                                            <a href="Upload.aspx">Upload Data</a>
                                            <a href="Users.aspx">Users</a>
                                            <a href="CreateUser.aspx">Create User</a>
                                        </div>
                                    </div>
                                    <div class="dropdown" runat="server" id="divAssets">
                                        <button class="dropbtn" disabled>
                                            Assets 
                                <i class="fa fa-caret-down"></i>
                                        </button>
                                        <div class="dropdown-content">
                                            <a href="Hosts.aspx">Hosts</a>
                                            <a href="Applications.aspx">Applications</a>
                                            <a href="Storage.aspx" runat="server" id="mnServers">Storage</a>
                                            <a href="Databases.aspx">Databases</a>
                                            <a href="Relationships.aspx">Relationships</a>
                                            <a href="Bundles.aspx">Bundles</a>
                                        </div>
                                    </div>
                                    <div class="dropdown">
                                        <button class="dropbtn" disabled>
                                            Reports 
                                <i class="fa fa-caret-down"></i>
                                        </button>
                                        <div class="dropdown-content">
                                            <a href="Templates.aspx">Download Templates</a>
                                        </div>
                                    </div>
                                    <a href="Preferences.aspx">Preferences</a>
                                </div>
                            </td>
                            <td style="text-align: right;">
                                <asp:Label ID="lblLoginUser" runat="server" Font-Bold="true" Font-Size="Small" ForeColor="#565656"></asp:Label>
                                |
                                <asp:LinkButton ID="lnkLogout" runat="server" OnClick="lnkLogout_Click" ForeColor="#565656" Font-Size="Small" Font-Italic="true">Logout</asp:LinkButton>
                                <br />
                                <br />
                                </dropdownlist><asp:Label ID="Label1" runat="server" Text="Project : "></asp:Label>
                                <asp:DropDownList ID="drpProject" runat="server" AutoPostBack="True" OnSelectedIndexChanged="drpProject_SelectedIndexChanged">
                                </asp:DropDownList>
                            </td>
                        </tr>

                    </table>
                    <hr class="Menuline">
                </div>
                <div id="dvProgressBar" style="visibility: hidden; margin: 0px; padding: 0px; position: fixed; right: 0px; top: 0px; width: 100%; height: 100%; background-color: white; z-index: 30001; opacity: 0.7;">
               <p style="position: absolute; color: White; top: 50%; left: 50%;"> <asp:Image runat="server" AlternateText="Please wait.." ImageUrl="~/Images/ajax-loader.gif"/>
                </p>
            </div>
            </ContentTemplate>
        </asp:UpdatePanel>

        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
    </form>
</body>
</html>
